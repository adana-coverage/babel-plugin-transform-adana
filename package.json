{
  "name": "babel-plugin-transform-adana",
  "version": "0.4.0",
  "description": "A better instanbul.",
  "repository": "izaakschroeder/babel-plugin-transform-adana",
  "license": "CC0-1.0",
  "main": "index.js",
  "keywords": [
    "coverage",
    "istanbul",
    "babel-plugin",
    "babel"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ignore-path .gitignore .",
    "prepublish": "./node_modules/.bin/babel -s inline -d dist src/",
    "prespec": "npm run prepublish && ./node_modules/.bin/babel --plugins ./,transform-es2015-modules-commonjs -d dist src/",
    "spec": "NODE_ENV=test ./node_modules/.bin/_mocha -r dump.js --slow 200 --compilers js:babel-core/register -R spec test/spec",
    "test": "npm run lint && npm run spec"
  },
  "devDependencies": {
    "adana-analyze": "^0.1.0",
    "adana-cli": "^0.1.0",
    "adana-format-lcov": "^0.1.0",
    "babel-cli": "^6.0.2",
    "babel-plugin-syntax-object-rest-spread": "^6.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.0.2",
    "babel-preset-es2015": "^6.0.11",
    "babylon": "^6.3.26",
    "chai": "^3.4.0",
    "eslint": "^1.8.0",
    "eslint-config-metalab": "^0.3.0",
    "mocha": "^2.2.5"
  },
  "peerDependencies": {
    "babel-core": ">=6.0.20"
  },
  "dependencies": {
    "babel-literal-to-ast": "^0.1.2",
    "babel-template": "^6.0.14",
    "minimatch": "^3.0.0",
    "mkdirp": "^0.5.1"
  }
}
